include h2;\r\n    color: $grey; \r\n    width: 50px;\r\n}\r\n.todo-list {\r\n    position: relative;\r\n    width: 1170px;\r\n}\r\n\r\n.todo-list-wrapper {\r\n    margin: 30px auto;\r\n    display: flex;\r\n    justify-content: space-around;\r\n\r\n}\r\n.make-todo-column  {\r\n    width: 360px;\r\n}\r\n\r\n.title {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n    padding: 18px 0;\r\n    box-shadow: 6px 3px 10px 2px rgba(0, 0, 0, .1);\r\n    border-radius: 10px;\r\n    background-color: #fff;\r\n}\r\n h3 {\r\n    @include h3;\r\n    color: $black;   \r\n}\r\n.option {\r\n    display: flex;\r\n}\r\n.option div{\r\n    margin-left: 20px;\r\n}\r\n\r\n.option-plus-img img{\r\n    width: 20px;\r\n    height: 20px; \r\n    cursor: pointer;  \r\n}\r\n.delete p {\r\n color: $blue;   \r\n}\r\n\r\n.option-delete-img img{\r\n    width: 20px;\r\n    height: 20px;\r\n    cursor: pointer;     \r\n}\r\n.in-progress-column {\r\n    width: 360px;\r\n}\r\n.done-column {\r\n    width: 360px;\r\n}\r\n\r\n.total {\r\n    margin: 10px 0 0 0;\r\n    padding: 10px 0;\r\n    display: flex;\r\n    justify-content: space-around;\r\n    box-shadow: 6px 3px 10px 2px rgba(0, 0, 0, .1);\r\n    border-radius: 10px;\r\n    background-color: #fff;\r\n    @include total;\r\n}\r\n\r\n.tasks-list {\r\n    margin: 10px 0 0 0;\r\n    padding: 15px;\r\n    box-shadow: 6px 3px 10px 2px rgba(0, 0, 0, .1);\r\n    background-color: #fff;\r\n    border-radius: 10px;\r\n\r\n}\r\n.task-date {\r\n    display: flex;\r\n    justify-content: space-around;\r\n}\r\n\r\n.task {\r\n    margin-top: 10px;\r\n    min-height: 200px;\r\n    min-width: 330px;\r\n    padding: 10px;\r\n    background-color: #e6f1f1;\r\n    border-radius: 10px;\r\n    box-shadow: 6px 3px 10px 2px rgba(0, 0, 0, .1);\r\n}\r\n\r\n.task .task-wrapper {\r\n    margin-left: 5px;\r\n    min-height: 90px;\r\n    width: 300px;\r\n    position: relative;\r\n}\r\n.task .task-nd {\r\n    min-height: 90px;\r\n    display: flex;\r\n    flex-direction: column;\r\n\r\n}\r\n.task-name {\r\n    @include task-name;\r\n    \r\n}\r\n.task .task-nd .task-descr{\r\n    margin-top: 10px;\r\n    font-size: 14px;\r\n}\r\n.task button {\r\n    // position: absolute;\r\n    box-sizing: border-box;\r\n    height: 20px;\r\n    width: 80px;\r\n    bottom: 0;\r\n    right: 0;\r\n    border: none;\r\n    background-color: #c468e8;\r\n    color: $white;\r\n    border-radius: 10px;\r\n    cursor: pointer;  \r\n}\r\n.task hr {\r\n    margin: 15px;\r\n}\r\n.task .delete {\r\n    display: flex;\r\n    justify-content: space-around;\r\n}\r\n.delete p {\r\n    font-family: 'Raleway';\r\n    font-style: bold;\r\n    font-weight: 500;\r\n    font-size: 17px;\r\n   }\r\n\r\n\r\n.option-delete {\r\n    display: flex;\r\n    margin-left: 20px;\r\n}\r\n.option-delete-img img{\r\n    width: 20px;\r\n    height: 20px;  \r\n    cursor: pointer;   \r\n}\r\n\r\n.pop-up {\r\n    position:fixed;\r\n    top:0;\r\n    left:0;\r\n    z-index:1050;\r\n    display: none;\r\n    width:100%;\r\n    height:100%;\r\n    overflow:hidden;\r\n    background-color:rgba(0,0,0,.5);\r\n\r\n}\r\n.pop-up-wrapper {\r\n    top: 100px;\r\n    left: 25%;\r\n    width: 600px;\r\n    height: 400px;\r\n    position: absolute;\r\n    display: flex;\r\n    flex-direction: column;\r\n    background-color: white;\r\n    padding: 0 15px 0 15px;\r\n    border-radius: 10px;\r\n    box-shadow: 6px 3px 10px 2px rgba(0, 0, 0, .1);\r\n}\r\n.pop-up .close {\r\n    border-radius: 10px;\r\n    width: 18px;\r\n    margin: 0;\r\n}\r\n.pop-up * {\r\n    margin: 8px;\r\n}\r\n\r\n//.pop-up input {\r\n//    border: 0.5px solid #8d9498;\r\n//    border-radius: 5px;\r\n//    padding: 5px 10px 5px 10px;\r\n//    background-color: #ececec;\r\n//}\r\n.pop-up .pop-descr {\r\n    height: 50px;\r\n}\r\n.pop-up form {\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\n.show{display:block}\r\n.hide{display:none}\r\n.error{border: 0.5px solid red;}","sas